{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "5.0.*",
		"milon/barcode": "5.3.2",
		"picqer/php-barcode-generator": "^0.3.0",
		"khanamiryan/qrcode-detector-decoder": "^1.0",
		"barryvdh/laravel-dompdf": "0.5.*",
		"phpmailer/phpmailer": "^6.9",
		"twilio/sdk": "~4.10",
		"pear/http_request2": "^2.6",
		"nexmo/client": "0.4.0",
        "guzzlehttp/guzzle": "5.3.4"
	},
	"repositories": [
        {
            "type": "path",
            "url": "D:/wamp64/www/inventory/vendor_manual/nexmo-client-0.4.0"
        },
        {
            "type": "path",
            "url": "D:/wamp64/www/inventory/vendor_manual/guzzle-5.3.4"
        }
    ],
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist",
		"allow-plugins": {
			"kylekatarnls/update-helper": true
		},
		"process-timeout": 600
	}
}
